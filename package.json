{
  "name": "benchwrk-open",
  "displayName": "BenchWrk",
  "description": "Logs aggregator",
  "type": "module",
  "version": "0.0.4",
  "publisher": "Benchwrk",
  "repository": {
    "url": ""
  },
  "icon": "icon128.png",
  "engines": {
    "vscode": "^1.103.0"
  },
  "activationEvents": [
    "onView:benchwrk.customPanel"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "benchwrk.showPanel",
        "title": "Show BenchWrk Panel"
      }
    ],
    "viewsContainers": {
      "panel": [
        {
          "id": "benchwrkPanel",
          "title": "BenchWrk",
          "icon": "$(terminal)"
        }
      ]
    },
    "views": {
      "benchwrkPanel": [
        {
          "type": "webview",
          "id": "benchwrk.customPanel",
          "name": "BenchWrk Panel",
          "contextualTitle": "BenchWrk"
        }
      ]
    }
  },
  "scripts": {
    "compile": "bun build.js",
    "build": "bun build.js",
    "release": "standard-version && vsce package",
    "watch": "bun --watch build.js",
    "dev": "bun build.js && code --new-window --extensionDevelopmentPath=./",
    "pretest": "bun run build && bun run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.103.0",
    "css-loader": "^7.1.2",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.2",
    "@tailwindcss/vite": "^4.1.12",
    "@types/node": "^24.3.0",
    "@types/react": "^18.2.58",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react-swc": "^4.0.1",
    "vite": "^7.1.3"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^4.7.0",
    "@vscode-elements/elements": "^2.3.0",
    "@vscode-elements/react-elements": "^2.3.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "immer": "^10.1.3",
    "radix-ui": "^1.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.12",
    "tw-animate-css": "^1.3.7",
    "uuid": "^11.1.0",
    "zustand": "^4.5.6"
  }
}
